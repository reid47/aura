{
  "name": "aura",
  "version": "1.0.0",
  "description": "An accessible, beginner-friendly browser-based code editor",
  "main": "index.js",
  "repository": "https://github.com/reid47/aura",
  "author": "Reid Mitchell <reidmitchell1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "build-watch": "rollup -c -w",
    "dev-server": "live-server --port=4747",
    "dev": "concurrently \"npm run build-watch\" \"npm run dev-server\""
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "babylon": "^6.18.0"
  }
}
